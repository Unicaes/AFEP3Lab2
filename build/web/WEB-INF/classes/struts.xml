<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="Model" extends="struts-default">
		<action name="registeruser" class="Model.RegisterAction">
			<result name="REGISTER">/register.jsp</result>
		</action>
		<action name="report" class="Model.ReportAction">
			<result name="REPORT">/report.jsp</result>
		</action>
		<action name="GraphicReport" class="Model.GraphicAction">
			<result name="GRAPHIC_REPORT">/Graphic.jsp</result>
		</action>
		<action name="deleterecord" class="Model.DeleteAction">
			<result name="DELETE">/delete.jsp</result>
		</action>
                
	</package>
</struts>